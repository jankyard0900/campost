<%= render "admin/shared/error_messages", resource: resource %>
<%= form_with model: model, url: url do |f| %>
  <span class="mr-5"><b><%= f.label :name, name %></b></span>
  <span class="mr-3"><%= f.text_field :name %></span>
  <span class="mr-5"><%= f.submit submit, class: 'btn btn-success' %></span>
<% end %>