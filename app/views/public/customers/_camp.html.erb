<!--会員のキャンプ場投稿一覽-->
<div class="col-md-4">
  <div class="row">
    <h5 class="mt-5 text-center", style="background-color: #eee; width:240px;"><b>投稿したキャンプ場</b></h5>
  </div>
  <% camps.each do |camp| %>
  <div class="row mt-3 ml-3">
    <%= link_to camp_path(camp.id), class: 'text-dark' do %>
      <u><%= camp.name %></u>
    <% end %>
  </div>
  <hr>
  <% end %>
</div>

<!--会員のキャンプギア投稿一覽-->
<div class="col-md-4">
  <div class="row">
    <h5 class="mt-5 text-center", style="background-color: #eee; width:220px;"><b>投稿したキャンプギア</b></h5>
  </div>
  <% gears.each do |gear| %>
  <div class="row mt-3 ml-3">
    <%= link_to gear_path(gear.id), class: 'text-dark' do %>
      <u><%= gear.name %></u>
    <% end %>
  </div>
  <hr>
  <% end %>
</div>
